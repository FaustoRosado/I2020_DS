    "import requests\n",
    "from bs4 import BeautifulSoup\n",
    "\n",
    "class Bot:\n",
    "    def __init__(self,url):\n",
    "        self.url = url\n",
    "        \n",
    "        info = Bot.get_req(url)\n",
    "        self.headers = info[0]\n",
    "        self.content = info[1]\n",
    "        self.status_code = info[2]\n",
    "    \n",
    "    def get_req(url):\n",
    "        resp = requests.get(url) \n",
    "        \n",
    "    \n",
    "        content = resp.content\n",
    "        status_code = resp.status_code\n",
    "        headers = resp.headers\n",
    "        \n",
    "        return [headers, content, status_code]\n",
    "             \n",
    "    \n",
    "    def get_tag(url,tag):\n",
    "        new_soup = BeautifulSoup(url)\n",
    "        t = new_soup.findAll(tag)\n",
    "        return t\n",
    "\n",
    "postman = Bot('https://postman-echo.com')\n",
    "\n",
    "print(postman.status_code)\n",
    "print(postman.headers)\n",
    "print(postman.content)\n",
    "\n"
   ]
