    "# Decorator can wrap itself like recursion\n",
    "\n",
    "def tagger(tag):\n",
    "    def decorator_function(func):        \n",
    "        def wrapper():\n",
    "             return f\"<{tag}>{func()}</{tag}>\"       \n",
    "        return wrapper   \n",
    "    return decorator_function\n",
    "\n",
    "@tagger(\"p\")\n",
    "@tagger('div')\n",
    "@tagger('b')\n",
    "@tagger('p')\n",
    "def hello():    \n",
    "    return \"hello\"\n",
